<!ELEMENT evaluation (#PCDATA)>
<!ELEMENT experimental_inst (installation, parameterization?, worksflow?, evaluation?, other_instructions?)>
<!ELEMENT file (fname, duration?, publisher_artifact_url?, description?, streaming?, caption_file?, poster_image?)>
<!ELEMENT first_name (#PCDATA)>
<!ELEMENT fname (#PCDATA)>
<!ELEMENT foreign_title (#PCDATA)>
<!ELEMENT format (#PCDATA)>
<!ELEMENT formats (format+)>
<!ELEMENT fulltext (file+)>
<!ELEMENT funder_identifier (#PCDATA)>
<!ELEMENT funding_agency (#PCDATA)>
<!ELEMENT general_inst (software_dependencies?, hardware_dependencies?, installation?, other_instructions?)>
<!ELEMENT grant_number (#PCDATA)>
<!ELEMENT grant_numbers (grant_number+)>
